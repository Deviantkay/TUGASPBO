package W;

/**
 *
 * @author Deviant Kay
 */
public class B {
    private final String name, caption, gender;
    private final int age;
    
    
    public B(String name, int age, String caption, String gender){
        this.name = name;
        this.age = age;
        this.caption = caption;
        this.gender = gender;
    }
        
    protected String getName() {
        return this.name;
    }
    
    protected String getCaption(){
        return this.caption;
    }
   
    protected int getAge() {
        return this.age;
    }
    
    protected String getGender(){
        return this.gender;
    }
}
